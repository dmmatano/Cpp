#include <iostream>
#include <algorithm>

using namespace std;

int main() {
  int mat[3][3], soma[]={0,0,0};
  for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
      cin>>mat[i][j];
      if(mat[i][j]%2!=0){
        soma[i]+=mat[i][j];
      }
    }
  }
  cout<<"Matriz criada:"<<endl;
  for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
      cout<<mat[i][j]<<" ";
    }
    cout<<endl;
  }
  cout<<"O vetor criado Ã©:"<<endl;
  for(int i=0;i<3;i++){
    cout<<soma[i]<<endl;
  }
  
}
