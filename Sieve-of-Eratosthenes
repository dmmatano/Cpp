#include <iostream>
using namespace std;
/**************** retorna todos os nums primos at√© o "n" elemento  ***********************/
bool e_composto[10000010];
void crivo(int n){
    e_composto[1]=1;
    for(int i=2;i<=n;i++){
        if(!e_composto[i]){
            for(int j=2;j*i<=n;j++){
                e_composto[i*j]=1;
            }
        }
    }
    return;
}
/*********************************************/
int main() {
  crivo(100);
  for(int i=0;i<=100;i++){
    cout<<e_composto[i]<<endl;
  }
}

